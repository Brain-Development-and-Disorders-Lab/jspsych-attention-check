{
  "name": "jspsych-attention-check",
  "version": "2.1.1",
  "description": "A jsPsych plugin for adding multiple-choice attention check questions to an experiment timeline.",
  "keywords": [
    "neuroscience",
    "jspsych",
    "psychology",
    "neuropsychology"
  ],
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "typings": "dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "repository": "https://github.com/Brain-Development-and-Disorders-Lab/jspsych-attention-check.git",
  "author": "Henry Burgess <henry.burgess@wustl.edu>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "tsc": "tsc",
    "build": "rollup --config",
    "build:watch": "npm run build -- --watch",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "jspsych": "^7.3.3"
  },
  "dependencies": {
    "grommet": "^2.33.2",
    "grommet-icons": "^4.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.1.0",
    "usehooks-ts": "^2.5.4"
  },
  "devDependencies": {
    "@jspsych/config": "^1.3.3",
    "@jspsych/test-utils": "^1.1.2",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "jspsych": "^7.3.3",
    "prettier": "^2.6.2",
    "process": "^0.11.10"
  }
}
